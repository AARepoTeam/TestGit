				<xsd:annotation>
					<xsd:documentation>Commission amount at this fare level
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="MarkUp" type="Amount.Details" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Applies to MSL only. Mark-up amount at this fare level.  This is the difference between the manual adjusted selling fare made by the agent and the adjusted selling fare returned by Pricing.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="AdjustedSelling" type="Amount.Details" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Applies to ASL only. Adjusted Selling fare amount at this level. This is the difference between the ASL and original fare amount.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="HandlingFee" type="Amount.Basic" minOccurs="0" maxOccurs="99">
				<xsd:annotation>
					<xsd:documentation>Handling fee associated to the adjusted selling fare
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
		<xsd:attribute name="manual" type="xsd:boolean">
			<xsd:annotation>
				<xsd:documentation>Set to true if manual adjusted selling fare</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
