			<xsd:element name="Customer" type="TicketingDocument.Customer" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Traveller or purchaser</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ServiceCoupon" type="TicketingDocument.ServiceCoupon.VOU" minOccurs="0" maxOccurs="30">
				<xsd:annotation>
					<xsd:documentation>A ticketing document can have one or more flight coupons</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Amounts" type="TicketingDocument.Amounts.VOU" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Any amounts associated to the ticketing document</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Taxes" type="TicketingDocument.Taxes.VOU" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Any taxes paid by the customer</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Remark" type="TicketingDocument.Remark" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>type=ENRS (Endorsement and Restriction - Mapped to VCR_60_EDR, VCR_60_EDM,
                        VCR_60_RES; type=VCR5 (Manually-added remarks) - Mapped to VCR_80_MAR;

                        Mapped to T2 database: TKT_DOC_RMK.TKT_DOC_RMK_TXT

                        Mapped to: #FS01_8030_EDM_023 (FS01_80_VAR)
                    </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="AffiliatedAgent" type="TicketingDocument.AffiliatedAgent.Ticket" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Other agents</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="RelatedDocument" type="TicketingDocument.Related.VOU" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Other Documents that are related to the primary document upon issuance
                    </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PrintCoupon" type="TicketingDocument.PrintCoupon" minOccurs="0" maxOccurs="6">
				<xsd:annotation>
					<xsd:documentation>Coupon information that are printed as part of ticketing transaction
                    </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Payment" type="TicketingDocument.Payment" minOccurs="0" maxOccurs="3">
				<xsd:annotation>
					<xsd:documentation>Two forms of payment are allowed. In case of exchanges, there could be another
                        form of paymemt code 'ET' that signifies the use of travel documents to issue another ticketing
                        document.
                    </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="History" type="TicketingDocument.History" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>History information associated to every transaction update made to the ticketing
                        document
                    </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
		<xsd:attribute name="type" type="Code.DocumentType" use="required">
			<xsd:annotation>
				<xsd:documentation>Ticketing document type issued by the agent

                    Mapped to T2 database: TKT_DOC.TKT_DOC_TYP_CD

                    Mapped to TPF file: #FS01_8030_IND_000 (FS01_80_IND_DOC)

                    Mapped to VCR record: Always set the value to TKT

                    Possible values can be:
                    TKT - Ticket
                    EMD - Electronic Miscellaneous Document
                    MSR - Miscellaneous Sales Receipt
                    EXB - Excess Baggage
                    VOU - Voucher
                    RFD - Refund
                    XXX - Cash Refund
                    CCR - Credit Card Refund
                </xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="number" type="Identifier.TicketingDocument" use="required">
			<xsd:annotation>
				<xsd:documentation>Ticketing document number dispensed and assigned by the system upon issuance; this is
                    the primary document number

                    Mapped to T2 database:
                    TKT_DOC.TKT_DOC_ID

                    Mapped to TPF file: #FS01_8030_TKT_026 (FS01_80_VAR)

                    Mapped to VCR record: VCR_40_TKN and VCR_40_TKD

                    Mapped to DM0Y record:
                    DM0Y_50_TACC and DM0Y_50_TNUM
                </xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="accountingCode" type="Identifier.TicketingProviderAccounting">
			<xsd:annotation>
				<xsd:documentation>Airline code identification number as defined by IATA

                    Mapped to T2 database:
                    TKT_DOC.TKT_PROVD_ACCT_ID

                    Mapped to TPF file: #FS01_8030_TKT_026 (FS01_80_TKT_AAC)

                    Mapped to VCR record: VCR_40_TAC

                    Mapped to DM0Y record:
                    DM0Y_50_TACC
                </xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="formNumber" type="Number.TicketingDocumentForm">
			<xsd:annotation>
				<xsd:documentation>Number representing the type of document form as defined by IATA

                    Mapped to T2 database:
                    TKT_DOC.TKT_DOC_FORM_NBR;

                    Mapped to TPF file: #FS01_8030_TKT_026 (FS01_80_TKT_FMN)

                    Mapped to VCR record: VCR_40_FMN
                </xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="serialNumber" type="Number.TicketingDocumentSerial">
			<xsd:annotation>
				<xsd:documentation>Number assigned on a sequential basis to make the document number as unique as
                    possible

                    Mapped to T2 database: DB-TKT_DOC.TKT_DOC_SN

                    Mapped to TPF file: #FS01_8030_TKT_026 (FS01_80_TKT_TNR)

                    Mapped to VCR record: VCR_40_TNR

                    Mapped to DM0Y record:
                    DM0Y_50_TNUM
                </xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="checkDigit" type="Number.CheckDigit">
			<xsd:annotation>
				<xsd:documentation>A digit calculated based on the document number modulus 7 for fraud purposes

                    Mapped to T2 database:
                    TKT_DOC_CHK_DIGT_NBR

                    Mapped to TPF file: #FS01_8030_TKT_026 (FS01_80_TKT_TCD)

                    Mapped to VCR record: VCR_40_ACD

                    Mapped to DM0Y record:
                    DM0Y_55_TCKD (1st coupon)
                </xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="service" type="Code.ServiceType">
			<xsd:annotation>
				<xsd:documentation>A code associated to the general service category type to determine the reason why
                    the document is issued. The value could be related to A - AIR (VCR and non-VCR) or R - RAIL (AMTRAK)

                    Mapped to T2 database:
                    TKT_DOC.SVC_TYP_CD

                    Mapped to TPF file: #FS01_80_HDR_100 (FS01_80_HDR_TST)

                    Mapped to VCR record: Always set value to 'A'
                </xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="Ticketing.SearchParameters">
		<xsd:sequence>
			<xsd:element name="TicketingProvider" type="Code.TicketingProviderCode">
				<xsd:annotation>
					<xsd:documentation>
                        Airline or Carrier Code

                        Mapped to TPF file: #FS01_8000_HDR_000 #FS01_80_HDR_100 (FS01_80_HDR_CLN)

                        Mapped to T2 database: TKT_TNA.TKT_PROVD_CD
                    </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DocumentNumber" type="Identifier.TicketingDocument" minOccurs="0" maxOccurs="4">
				<xsd:annotation>
					<xsd:documentation>
                        Ticketing document number dispensed and assigned by the system upon issuance; this is the
                        primary document number

                        Mapped to T2 database: TKT_DOC.TKT_DOC_ID

                        Mapped to TPF File: #FS01_8030_TKT_026 (FS01_80_VAR)
                    </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="LocalIssueDate" type="xsd:date" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
                        The local date the agent issued the document

                        Mapped to T2 database: TKT_DOC.TKT_DOC_LCL_ISSUE_DTM

                        Mapped to TPF File: #FS01_8030_DTE_069 (FS01_80_DAT_DTE, FS01_80_DAT_TIM)
                    </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="WorkLocation" type="Identifier.LocationExtended" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
                        The location where the agent issued the ticketing document. This is normallly the point of sale
                        or AAA city.

                        Mapped to T2 database: TKT_TNA.ISSUING_AGT_WRK_LOC_ID;

                        Mapped to TPF file: '#FS01_8010_AAA_011 (FS01_80_CTY_CTY)
                    </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="FirstName" type="ProperName.29Character" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
                        The customer's first name.

                        Mapped to T2 database: TKT_DOC_CST.CST_FRST_NM;

                        Mapped to TPF File: #FS01_8020_NMG_004 (FS01_80_VAR)
                    </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="LastName" type="ProperName.29Character" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
                        The customer's last name.

                        Mapped to T2 database: TKT_DOC.CST_LAST_NM;

                        Mapped to TPF File: #FS01_8020_NML_003 (FS01_80_VAR)
                    </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Reservation" type="Reservation.SearchType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
                        The code that identifies the customer's reservation record

                        Mapped to T2 database: TKT_DOC.SAB_RES_ID

                        Mapped to TPF File: #FS01_8030_PNR_018 (FS01_80_PNR_LOC)
                    </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="FrequentFlyerNumber" type="Identifier.Affinity" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
                        The customer's loyalty number assigned by the airline

                        Mapped to T2 database: AFF_ACCT.AFF_ACCT_ID and AFF_ACCT.AFF_ACCT_TYP_CD
                    </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ProfileNumber" type="Identifier.Affinity" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
                        The customer's profile number assigned by Sabre

                        Mapped to T2 database: AFF_ACCT.AFF_ACCT_ID and AFF_ACCT.AFF_ACCT_TYP_CD
                    </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="AfterIssueDate" type="xsd:date" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
                        The local date the agent issued the document. Required for date-range search

                        Mapped to T2 database: TKT_DOC.TKT_DOC_LCL_ISSUE_DTM
                    </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="BeforeIssueDate" type="xsd:date" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
                        The local date the agent issued the document. Required for date-range search

                        Mapped to T2 database: TKT_DOC.TKT_DOC_LCL_ISSUE_DTM
                    </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DocumentType" type="Code.DocumentType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
                        Ticketing document type issued by the agent

                        Mapped to T2 database: TKT_DOC.TKT_DOC_TYP_CD
                    </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SystemCreateDate" type="xsd:date" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attribute name="resultType" type="Code.GetTicketingResultType"/>
	</xsd:complexType>
	<xsd:complexType name="Ticketing.TransactionInfo">
		<xsd:complexContent>
			<xsd:extension base="STL:TransactionInfo">
				<xsd:sequence>
					<xsd:element name="TCN" type="Number.TicketingTransactionControl" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Transaction Control Number, A random number assigned to the transaction
                                by the host system.

                                Mapped to T2 database:
                                TKT_TNA.TCN

                                Mapped to TPF File: #FS01_8030_TCN_025 (FS01_80_VAR)

                                Mapped to DM0Y:
                                DM0Y_45_TCNR
                            </xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="LocalDateTime" type="xsd:dateTime">
						<xsd:annotation>
							<xsd:documentation>Date and time when the agent issued the ticketing transaction.

                                Mapped to T2 database:
                                TKT_TNA.TNA_LCL_DTM

                                Mapped to TPF File: #FS01_8000_HDR_000
                                #FS01_80_HDR_100 (FS01_80_HDR_DTE, FS01_80_HDR_TIM)
                            </xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="SystemDateTime" type="xsd:dateTime">
						<xsd:annotation>
							<xsd:documentation>Date and time that the document was created in the ticketing system (Host
                                PSS).
                                Mapped to TPF File: #FS01_8000_TDT_001 (FS01_80_DAT_DTE, FS01_80_DAT_TIM)

                                Mapped to T2 database:
                                TKT_TNA.TKT_SYS_DTM

                                Mapped to DM0Y record:
                                DM0Y_E0_DOTA and DM0Y_E0_TIME
                            </xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="SystemProvider" type="Identifier.Provider">
						<xsd:annotation>
							<xsd:documentation>Global distribution system within the Sabre System or a carrier
                                reservation system that issued a document.


                                Mapped to T2 database:
                                TKT_TNA.SYS_PROVD_CD

                                Mapped to TPF file: #FS01_8010_AGT_010 (FS01_80_AGT_CLN)

                                Mapped to VCR record: VCR_40_CRS

                                Mapped to DM0Y record:
                                DM0Y_45_CRSI
                            </xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="InputEntry" type="String.Message" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Input entry initiated for this transaction that generates the ticketing
                                document

                                Mapped to T2 database:
                                TKT_TNA.ISSUING_AGT_INPUT_TXT

                                Mapped to TPF file: #FS01_80E0_IMG_001 (FS01_80_VAR)

                                Mapped to VCR: VCR_D0_IPM

                                Mapped to DM0Y record:
                                DM0Y_E0_IPMS
                            </xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
				<xsd:attribute name="sequence" type="xsd:integer">
					<xsd:annotation>
						<xsd:documentation>A number received from the host system that identifies the sequence that this
                            transaction is sent. It is optional and only applies to the VCR where the system of record
                            is PSS.

                            Mapped to T2 database: TKT_TNA.TNA_SEQ_NBR

                            Mapped to TPF file: #FS01_80_HDR_100 (FS01_80_HDR_TSN)

                            Mapped to VCR record: VCR_D1_SEQ
                        </xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
				<xsd:attribute name="category" type="Code.TransactionCategoryType">
					<xsd:annotation>
						<xsd:documentation>A code assigned to a transaction charaterized by a certain category or group
                            of transactions that follow the same process where the only difference is the data received.

                            Mapped to T2 database:
                            TKT_TNA.TKT_TNA_CAT_CD

                            Mapped to TPF file: TBD
                        </xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
				<xsd:attribute name="type" type="Code.TicketingTransactionType">
					<xsd:annotation>
						<xsd:documentation>A code associated to the transaction type as received from PSS host system.

                            Values can be:
                            SALE
                            EXCH
                            RFND
                            ARMK
                            STAT
                            PNRA
                            VOID

                            Mapped to T2 database:
                            TKT_TNA.TKT_TNA_TYP_CD

                            Mapped to TPF file: #FS01_80_HDR_100 (FS01_80_HDR_TYP)
                        </xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
				<xsd:attribute name="code" type="Code.TicketingTransactionType">
					<xsd:annotation>
						<xsd:documentation>A code associated to the transaction as defined by IATA

                            Mapped to T2 database:
                            TKT_TNA.TKT_TNA_EVN_CD

                            Mapped to TPF file: TBD
                        </xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
				<xsd:attribute name="actionType" type="Code.DocumentEventActivityType">
					<xsd:annotation>
						<xsd:documentation>A code used to apply additional processing for a document update or
                            retrieval. For example, if the transaction is to change the coupon status, EMD can also be
                            associated/disassociated in addition to the status change. If the transaction is get info
                            about the document, an abbreviated response can be returned or processed.

                            A - associate a document
                            D - disassociate a document
                            G - abbreviated get
                            L - process linked documents
                        </xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="Ticketing.TransactionInfo.History">
		<xsd:complexContent>
			<xsd:extension base="STL:TransactionInfo">
				<xsd:sequence>
					<xsd:element name="LocalDateTime" type="xsd:dateTime" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Date and time when the agent issued the ticketing transaction.
                            </xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="SystemDateTime" type="xsd:dateTime">
