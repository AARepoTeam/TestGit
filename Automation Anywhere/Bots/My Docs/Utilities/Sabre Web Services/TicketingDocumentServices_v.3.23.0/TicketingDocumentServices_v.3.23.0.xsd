	<xsd:complexType name="Amount.Basic.OBFee">
		<xsd:complexContent>
			<xsd:extension base="Amount.Basic">
				<xsd:sequence>
					<xsd:element name="FeeCode" type="Code.AmountValue">
						<xsd:annotation>
							<xsd:documentation>IATA defined OB Fee code also indicating fee type e.g. OBT01, OBR03,
                                OBFCA
                                Types defined by IATA:
                                OBT - Ticketing fees
                                OBR - Requested Service fees
                                OBF - Form of Payment fees

                                Mapped to T2 database:
                                TKT_AMT.AMT_VAL_CD
                                Mapped to VCRDTB:
                                VCR_5A_FEE_TYP
                                VCR_5A_FEE_COD
                            </xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="PricedCode" type="Code.PricingCode" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Indicates how fee was priced, possible values are:
                                0 - system priced
                                1 - agent priced

                                Mapped to T2 database:
                                TKT_AMT.PRICED_CD

                                Mapped to VCRDTB:
                                VCR_5A_FEE_SMO
                            </xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="WaiverCode" type="Identifier.Waiver" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Waiver reason code as defined by the airline in MISCO* table

                                Mapped to T2 database:
                                TKT_AMT.WAIVE_CD

                                Mapped to VCRDTB:
                                VCR_5A_FEE_WCD
                            </xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="Description" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Fee description as defined by IATA or Airline in the pricing system

                                Mapped to T2 database:
                                TKT_AMT.DSCR_TXT

                                Mapped to VCRDTB:
                                VCR_5A_FEE_DLN, VCR_5A_FEE_DST
                            </xsd:documentation>
						</xsd:annotation>
						<xsd:simpleType>
							<xsd:restriction base="xsd:string">
								<xsd:minLength value="1"/>
								<xsd:maxLength value="40"/>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:element>
				</xsd:sequence>
				<xsd:attribute name="sequence" type="xsd:integer" use="required">
					<xsd:annotation>
						<xsd:documentation>Sequence number of fees in document

                            Mapped to T2 database:
                            TKT_AMT.MISC_ITM_SEQ_NR

                            Mapped to VCRDTB:
                            VCR_5A_FEE_IDX
                        </xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
