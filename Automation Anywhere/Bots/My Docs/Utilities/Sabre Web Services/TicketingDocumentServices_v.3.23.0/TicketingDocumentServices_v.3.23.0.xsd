				<xsd:attribute name="associationType" type="Code.AssociationType" use="required"/>
				<xsd:attribute name="couponNumber" type="Number.CouponSequence" use="required"/>
				<xsd:attribute name="checkDigit" type="Number.CheckDigit"/>
			</xsd:extension>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="Number.Document">
		<xsd:simpleContent>
			<xsd:extension base="STL:StringLength1to15">
				<xsd:attribute name="couponNumber" type="STL:CouponNumber"/>
			</xsd:extension>
		</xsd:simpleContent>
	</xsd:complexType>
