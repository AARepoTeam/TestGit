	<xsd:complexType name="TourCodeSAC">
		<xsd:simpleContent>
			<xsd:extension base="xsd:string">
				<xsd:attribute name="suppressFareReplaceWithBT" type="xsd:boolean">
					<xsd:annotation>
						<xsd:documentation>Replace suppressed fare calculation line  with BT (EG: WUB*TEST1231 equates to TourCode text="TEST1231" suppressFareReplaceWithBT="true")</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
				<xsd:attribute name="suppressFareReplaceWithIT" type="xsd:boolean">
					<xsd:annotation>
						<xsd:documentation>Replace suppressed fare calculation line with IT (EG: WUI*TEST1231 equates to TourCode text="TEST1231" suppressFareReplaceWithIT="true")</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
				<xsd:attribute name="suppressIT" type="xsd:boolean">
					<xsd:annotation>
						<xsd:documentation>Suppress the IT from the tour code box allowing for a longer tour code (EG: WUN*TEST1231 equates to TourCode text="TEST1231" suppressIT="true")</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
				<xsd:attribute name="suppressITSuppressFare" type="xsd:boolean">
					<xsd:annotation>
						<xsd:documentation>Suppress fare and suppress IT in tour code box (EG: WUX*TEST1231 equates to TourCode text="TEST1231" suppressITSuppressFare="true")</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
			</xsd:extension>
		</xsd:simpleContent>
