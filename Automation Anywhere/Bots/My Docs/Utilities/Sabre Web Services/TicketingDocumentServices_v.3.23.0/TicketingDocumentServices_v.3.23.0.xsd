			<xsd:element name="Authorized" type="Amount.Basic" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Authorized amount (CC)

                        Mapped to T2 database: BANK_CARD.AUTH_AMT_TXT
                    </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="AddressVerification" type="Code.AddressVerification" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Address Verification (CC)

                        Mapped to T2 database: BANK_CARD.ADR_VER_CD
                    </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="TransactionId" type="Identifier.Transaction" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Credit card transaction information

                        Mapped to T2 database:
                        BANK_CARD.BANK_CARD_TNA_ID

                        Mapped to VCR Record:
                        VCR_55_TRD

                        Mapped to FS01 Record:
                        FS01_80_CCN_TRI
                    </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="CorporateContract" type="Code.CorporateContract" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Credit card corporate contract

                        Mapped to T2 database: BANK_CARD.CORP_CONTRCT_CD
                    </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DynamicCurrencyConversion" type="DynamicCurrencyConversion" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Details related to payment amount conversion to the credit card currency code

                        Mapped to T2 Datavase (BANK_CARD table):
                        @decimalPlace - DCC_ DCML_PLC
                        Amount - DCC_TOT_AMT
                        Amount/@currencyCode - DCC_CRNCY_CD
                        Amount/@decimalPlace - DCC_TOT_AMT_DCML_PLC
                        Text - DCC_TOT_AMT_TXT
                        Rate - DCC_RT
                        Rate/@decimalPlace - DCC_ RT_DCML_PLC

                        Mapped to VCR record:
                        @decimalPlace - VCR_66_DDML
                        Amount - VCR_66_DAMT
                        Amount/@currencyCode - VCR_66_DCUR
                        Amount/@decimalPlace - VCR_66_DAMT
                        Text - VCR_66_DAMT
                        Rate - VCR_66_DROE
                        Rate/@decimalPlace - VCR_66_DROE

                        Mapped to FS01 record:
                        EMD Issuance  DCC Info
                        #FS01_8070_FDCC_007 EQU 7 ! DCC FOP DATA (VAR)
                        * --> #FS01_8070_FDCC_007 ----> @FS01_80_AMT_4

                        EMD and Refund Doc  DCC Info
                        Additions in 8080 to include DCC data for Refund Document.
                        #FS01_8080_DCC_014 EQU 14 ? DCC RELATED DOC
                        * @ FS01_80_AMT_4
                    </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
		<xsd:attribute name="accb" type="Code.Accb">
			<xsd:annotation>
				<xsd:documentation>Mapped to T2 database: BANK_CARD.XLR8_BANK_CARD_BLL_IND

                    Mapped to VCR record:
                    VCR_55_BIL
                </xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="cardType" type="Code.OTA">
			<xsd:annotation>
				<xsd:documentation>Mapped to T2 database: BANK_CARD.BANK_CARD_VND_CD

                    Mapped to VCR record:
                    VCR_55_CCV

                    Indicates the type of magnetic striped card.
                    Refer to OTA Code ListCard Type (CDT).
                </xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="cardName" type="String.FormOfPayment"/>
	</xsd:complexType>
	<xsd:complexType name="Payment.BankCard.Pir">
		<xsd:sequence>
			<xsd:element name="MaskedCardNumber" type="Number.CreditCard">
				<xsd:annotation>
					<xsd:documentation>Credit card number masked due to PCI</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ExpireDate" type="Expire_Date" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Crediit card expiration date</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ApprovalCode" type="Code.Approval" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
                    </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ExtendedPayment" type="Code.ExtendedPayment" minOccurs="0"/>
			<xsd:element name="DynamicCurrencyConversion" type="DynamicCurrencyConversion" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attribute name="cardType" type="Code.OTA" use="required">
			<xsd:annotation>
				<xsd:documentation>Indicates the type of magnetic striped card. The same as credit card vendor.
                </xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="cardName" type="String.FormOfPayment"/>
	</xsd:complexType>
	<xsd:complexType name="Payment.BankCheck">
		<xsd:sequence>
			<xsd:element name="RoutingNumber" type="xsd:integer" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Represents the numeric code identifying the financial institution for a specific
                        account.

                        Currently not used in Ticketing.
                    </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ApprovalCode" type="Code.CheckApproval" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>A series of characters assigned by the applicable Bank authorization system to
                        confirm the approval of a sale transaction paid with check.

                        Mapped to T2 database: TKT_DOC_PAYMNT.PAYMNT_APRV_ID
                    </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="Payment.Cash">
		<xsd:attribute name="CashIndicator" type="xsd:boolean" default="true">
			<xsd:annotation>
				<xsd:documentation>If true, this indicates cash is being used.</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="Payment.Certficate">
		<xsd:sequence>
			<xsd:element name="Number" type="STL:StringLength1to14" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="Payment.DirectBill">
		<xsd:sequence>
			<xsd:element name="Company" type="Vendor" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Company name to whom remittance should be directed.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ContactName" type="String.ProperName" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>This may be used to pass the name of the contact at the company for which the
                        direct bill applies.
                    </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Address" type="STL:Address.Detailed" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Address where remittance should be directed.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
		<xsd:attribute name="ID" type="String.Short">
			<xsd:annotation>
				<xsd:documentation>Identifier for the organization to be billed directly for travel services.
                </xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="Payment.GovernmentTravel">
		<xsd:sequence>
			<xsd:element name="Number" type="STL:StringLength1to13" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="Payment.Invoice">
		<xsd:sequence>
			<xsd:element name="Number" type="STL:StringLength1to14" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="Payment.Miscellaneous">
		<xsd:attribute name="MiscIndicator" type="xsd:boolean" default="true">
			<xsd:annotation>
				<xsd:documentation>If true, this indicates miscellaneous (unknown) payment is being used.
                </xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="Payment.Other">
		<xsd:sequence>
			<xsd:element name="Details" type="String.FormOfPayment" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="Payment.PrepaidTicketAdvise">
		<xsd:sequence>
			<xsd:element name="Number" type="STL:StringLength1to14" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="Payment.RevenueAccounting">
		<xsd:sequence>
			<xsd:element name="Number" type="STL:StringLength1to10" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="Payment.TravelDocument">
		<xsd:sequence>
			<xsd:element name="Number" type="xsd:decimal" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>An identifier for issued document.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Expiration" type="xsd:dateTime" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The last day an unused travel document may be used as a form of payment
                    </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Issue" type="xsd:dateTime" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The date the document was issued</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Amount" type="STL:Amount" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The amount that is available to be used as a form of payment for other
                        services.
                    </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="FormOfPayment" type="String.FormOfPayment" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The form of payment is not in the Sabre ticketing system. It is a manually
                        entered field used to track the form of payment for the document.
                    </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PlaceOfIssuance" type="String.ProperName" minOccurs="0"/>
			<xsd:element name="ServiceCoupon" type="String.ServiceCoupon" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Number of the ticket document coupon used as a form a payment.
                    </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="Payment.Voucher">
		<xsd:sequence>
			<xsd:element name="ExpireDate" type="xsd:date" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Mapped to T2 database: TKT_DOC_PAYMNT.PAYMNT_INSTMNT_EXPRDT</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SeriesCode" type="Code.VoucherSeries" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Not currently used</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="paymentRQDetails">
		<xsd:sequence>
			<xsd:element name="Card" minOccurs="0">
				<xsd:complexType>
					<xsd:attribute name="securityCode" type="STL:NumericStringLength1to8">
						<xsd:annotation>
							<xsd:documentation>Example for airline ticketing: the 456 portion of WF*BA4005550000000001/0619*C456. Ref: AirTicketRQ/OptionalQualifiers/FOP_Qualifiers/SabreSonicTicketing/BasicFOP/CC_Info/PaymentCard/@CardSecurityCode. For MFOP, this will map to securityCode under EnhancedMultipleFOP</xsd:documentation>
						</xsd:annotation>
					</xsd:attribute>
					<xsd:attribute name="code" type="STL:VendorCode">
						<xsd:annotation>
							<xsd:documentation>Vendor code. Example for airline ticketing: the BA portion of WF*BA4005550000000001/0619. Ref: AirTicketRQ/OptionalQualifiers/FOP_Qualifiers/SabreSonicTicketing/BasicFOP/CC_Info/PaymentCard/@Code. </xsd:documentation>
						</xsd:annotation>
					</xsd:attribute>
					<xsd:attribute name="expireDate" type="xsd:gYearMonth">
						<xsd:annotation>
							<xsd:documentation>Expiration date Accepted format: "yyyy-mm". Example for airline ticketing: the 0619 portion of WF*BA4005550000000001/0619. Ref: AirTicketRQ/OptionalQualifiers/FOP_Qualifiers/SabreSonicTicketing/BasicFOP/CC_Info/PaymentCard/@ExpireDate. For MFOP, this will map to ExpiryDate under EnhancedMultipleFOP</xsd:documentation>
						</xsd:annotation>
					</xsd:attribute>
					<xsd:attribute name="suppress" type="xsd:boolean">
						<xsd:annotation>
							<xsd:documentation>Tell whether the credit card number should be suppressed on an itierary or not. Used by subscribers. Example: -XN portion of WF*BA4005550000000001/0619-XN. Ref: AirTicketRQ/OptionalQualifiers/FOP_Qualifiers/SabreSonicTicketing/BasicFOP/CC_Info/@Suppress</xsd:documentation>
						</xsd:annotation>
					</xsd:attribute>
					<xsd:attribute name="number" type="Number.ClearCreditCard">
						<xsd:annotation>
							<xsd:documentation>Clear Credit card number. Example: 40055500000001 portion of WF*BA4005550000000001/0619. Ref: AirTicketRQ/OptionalQualifiers/FOP_Qualifiers/SabreSonicTicketing/BasicFOP/CC_Info/PaymentCard/@Number</xsd:documentation>
						</xsd:annotation>
					</xsd:attribute>
					<xsd:attribute name="tokenizedNumber" type="Number.CreditCard">
						<xsd:annotation>
							<xsd:documentation>Tokenized credit card number. No entry equivalent. After detokenization the returned number would go in the number attribute
Ref: AirTicketRQ/OptionalQualifiers/FOP_Qualifiers/SabreSonicTicketing/BasicFOP/CC_Info/PaymentCard/@Number</xsd:documentation>
						</xsd:annotation>
					</xsd:attribute>
					<xsd:attribute name="manualOBFee" type="xsd:string">
						<xsd:annotation>
							<xsd:documentation>Used by subscribers. Ref: AirTicketRQ/OptionalQualifiers/FOP_Qualifiers/BSP_Ticketing/MultipleFOP/FOP_One/CC_Info/PaymentCard/@ManualOBFee</xsd:documentation>
						</xsd:annotation>
					</xsd:attribute>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="Amount" type="Amount" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Currently only applicable to 2nd Payment element. Ignored in 1st payment element. Amount to apply to this payment. Example: 100.00 portion of  WFMPBA4005550000000001/0619*CK/100.00. Ref: AirTicketRQ/OptionalQualifiers/FOP_Qualifiers/SabreSonicTicketing/EnhancedMultipleFOP/Fare/@Amount (for the Amount attribute; the CurrencyCode and DecimalPlaces attributes don't map to AirTicketLLSRQ)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Tax" type="Tax.Sws" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>xPath - Ref: AirTicketRQ/sws:OptionalQualifiers/sws:FOP_Qualifiers/sws:SabreSonicTicketing/sws:MultipleFOP/sws:Taxes. Tax is applicable only for 2nd FOP. This will be mapped to ExhancedMultipleFOP if any of securityCode or extendedPay or expiryDate is present else will be mapped to Taxes under MultipleFOP. Note - Taxes are mandatory under ExhancedMultipleFOP for AirTicketRQ. If tax is not included, default it to XT for tax code and zero amount</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ExtendedPayment" type="PayLaterPlan" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Extended payment allows the customer to pay the price of the ticket  over a period of additional months.

ExtendedPayment is applicable for boht Airline and Subscriber.Additional element and attributes required such as Amount, Type, and/or Card data. Example: W$FMPCA*MSPL03XRG065X10000//200.00 where the CA portion is the type and the 200.00 portion is the amount. 
Ref: sws:AirTicketRQ/sws:OptionalQualifiers/sws:FOP_Qualifiers/sws:BSP_Ticketing/sws:PayLaterPlan/sws:Installment</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
		<xsd:attribute name="type" type="xsd:string">
			<xsd:annotation>
				<xsd:documentation>Payment type. Examples include:
PT - Prepaid Ticket
CA - Cash
CK - Check
CQ - Cheque
Ref: AirTicketRQ/OptionalQualifiers/FOP_Qualifiers/SabreSonicTicketing/BasicFOP/@Type</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="subtype" type="xsd:string">
			<xsd:annotation>
				<xsd:documentation>To immediately follow Type attribute in entry generated for PSS. Example assuming attribute Type="GTR": the 88776655 portion of WFGTR88776655. Ref: AirTicketRQ/OptionalQualifiers/FOP_Qualifiers/SabreSonicTicketing/BasicFOP/@Type (appended after value in Type attribute)</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="manualApprovalCode" type="STL:StringLength1to16">
			<xsd:annotation>
				<xsd:documentation>Non-credit card manual approval code. 
Example: the 1234 portion of WFCK*Z1234 Ref: AirTicketRQ/OptionalQualifiers/FOP_Qualifiers/SabreSonicTicketing/BasicFOP/@ManualApprovalCode
or for CC type of payment
AirTicketRQ/OptionalQualifiers/FOP_Qualifiers/SabreSonicTicketing/BasicFOP/CC_Info/PaymentCard/@ManualApprovalCode</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="misc" type="xsd:boolean">
			<xsd:annotation>
				<xsd:documentation>Form of payment or multiple form of payment type is preceded by MS in the entry being generated. If used, attribute Type is what immediately follows MS in the entry</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="reference">
			<xsd:annotation>
				<xsd:documentation>Item number of FOP data in the PNR to be used to pay for ticket. Example: value 1 generates WF*FOP1 Ref: AirTicketRQ/OptionalQualifiers/FOP_Qualifiers/BasicFOP/@Reference</xsd:documentation>
			</xsd:annotation>
			<xsd:simpleType>
				<xsd:restriction base="xsd:integer">
					<xsd:minInclusive value="1"/>
					<xsd:maxInclusive value="999"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
		<xsd:attribute name="virtual" type="xsd:string">
			<xsd:annotation>
				<xsd:documentation>Virtual is used to pass virtual form of payment information. Example: WF*VCNSABREAIRONETWOFOUR. Ref: sws:AirTicketRQ/sws:OptionalQualifiers/sws:FOP_Qualifiers/sws:BasicFOP/@Virtual</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="PayLaterPlan">
		<xsd:all>
			<xsd:element name="Installment" type="InstallmentType">
				<xsd:annotation>
					<xsd:documentation>Specifies the number of installments. Applicable for Airline and Subscriber. Example: 03 portion of 
W$FMPCA*MSPL03XRG065X10000//200.00.

WF*AXXXXXXXXXX5550/1215*EP20 
Ref:/sws:PayLaterPlan/sws:Installment/@Count

AirTicketRQ/OptionalQualifiers/FOP_Qualifiers/SabreSonicTicketing/BasicFOP/CC_Info/PaymentCard/@ExtendedPayment. For MFOP, this will map to extendedPayment under EnhancedMultipleFOP</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:all>
		<xsd:attribute name="referenceNumber" type="xsd:string" use="optional">
			<xsd:annotation>
				<xsd:documentation>Specifies the pay later plan reference number applicable for subscriber.
Example: XRG065 portion of W$FMPCA*MSPL03XRG065X10000//200.00. 
Ref:/sws:PayLaterPlan/sws:Installment/@PayLaterReferenceNumber</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="Phase4SAC">
		<xsd:sequence>
			<xsd:element name="SelectName" type="SelectNameType" minOccurs="0" maxOccurs="99">
				<xsd:annotation>
					<xsd:documentation>Identify applicable phase 4 names (EG: WT1N...)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
		<xsd:attribute name="number" use="required">
			<xsd:annotation>
				<xsd:documentation>Specify by number which phase 4 record to use (EG: WT1 equates to Phase4 number="1")</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="PQFilterType">
		<xsd:attribute name="all" type="xsd:boolean">
			<xsd:annotation>
				<xsd:documentation>Issue tickets for all active PQs for TN customer. If attribute is not present assume value is false.</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="lowCostCarrier" type="xsd:boolean">
			<xsd:annotation>
				<xsd:documentation>Process only LCC type PQs</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="eTkt" type="xsd:boolean">
			<xsd:annotation>
				<xsd:documentation>Process only eTkt type PQs (non-LCC)</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="PQRecord">
		<xsd:attribute name="number" type="xsd:integer" use="required">
			<xsd:annotation>
				<xsd:documentation>Specify PQ number (EG: WPQ1 equates to Record number="1")</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="endNumber" type="xsd:integer">
			<xsd:annotation>
				<xsd:documentation>Specify PQ numbers by range (EG: WPQ1-5 equates to Record number="1" endNumber="5")</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="reissue" type="xsd:boolean">
			<xsd:annotation>
				<xsd:documentation>Indicate when value is true that the reissue type of PQ (PQR) should be used (EG: WPQR1 equates to Record number="1" reissue="true"). Omission of attribute or a value of false assumes normal non-reissue PQ record</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="Preferences.Validate">
		<xsd:sequence>
			<xsd:element name="PassengerTypeOverride" type="PassengerTypeCodeType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Specify passenger type if it is different than the one stored in the ticket.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="CurrencyOverride" type="Code.CurrencyType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Override default pricing currency.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
		<xsd:attribute name="allowOADocumentExchange" type="xsd:boolean" default="true">
			<xsd:annotation>
				<xsd:documentation>If true (default) tells system to accept requests where TicketingProvider is marketing or operating carrier on at least one segment in exchanged document
If false then exchange document must be issued on TicketingProvider stock</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="PriceQuoteSAC">
		<xsd:sequence>
			<xsd:element name="Record" type="PQRecord" minOccurs="0" maxOccurs="99">
				<xsd:annotation>
					<xsd:documentation>Use specified PQ to price (EG: WPQ...)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SelectName" type="SelectNameType" minOccurs="0" maxOccurs="99">
				<xsd:annotation>
					<xsd:documentation>Limit the names applicable to PQ for ticketing (EG: WPQN)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="PricingCurrency.Validate">
		<xsd:attribute name="base" type="Code.CurrencyType">
			<xsd:annotation>
				<xsd:documentation>Base currency.</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="equiv" type="Code.CurrencyType">
			<xsd:annotation>
				<xsd:documentation>Equivalent currency.</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="PricingOptionsType">
		<xsd:sequence>
			<xsd:element name="Brand" type="BrandType" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Specify brand - not more than 1 brand per journey or 1 brand per itinerary segment. Example: WBRCP. Ref: AirTicketRQ/OptionalQualifiers/PricingQualifiers/Brand</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Itinerary" type="PricingOptionsItinerary" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Limit or characterize the itinerary being priced for ticketing. Ref: AirTicketRQ/OptionalQualifiers/PricingQualifiers/ItineraryOptions</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SelectName" type="SelectNameType" minOccurs="0" maxOccurs="99">
				<xsd:annotation>
					<xsd:documentation>Limit the names to price for ticketing (EG: WN). Ref: AirTicketRQ/OptionalQualifiers/PricingQualifiers/NameSelect</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Phase4" type="Phase4SAC" minOccurs="0" maxOccurs="7">
				<xsd:annotation>
					<xsd:documentation>Use phase 4 price information to issue ticket (EG: WT). Ref: AirTicketRQ/OptionalQualifiers/PricingQualifiers/PhaseIV</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PQFilter" type="PQFilterType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>TN only. PQ selection filter attributes. Directs orchestration module on which PQs to process. Omission of boolean attribute means the value is false.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PriceQuote" type="PriceQuoteSAC" minOccurs="0" maxOccurs="99">
				<xsd:annotation>
					<xsd:documentation>Use price quote (PQ) price information to issue ticket (EG: WPQ). Ref: AirTicketRQ/OptionalQualifiers/PricingQualifiers/PriceQuote</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Validity" type="ValiditySAC" minOccurs="0" maxOccurs="99">
				<xsd:annotation>
					<xsd:documentation>Specify valid ticket issue dates (EG: WV). Ref: AirTicketRQ/OptionalQualifiers/PricingQualifiers/ValidityDates</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
		<xsd:attribute name="fareType" type="STL:AlphaLength2">
			<xsd:annotation>
				<xsd:documentation>Equivalent of secondary action code T/ identifying the fare type. Valid values are NL for normal, EX for special and IT for inclusive tour. EG: WT/NL equates to fareType="NL". Used by Sabre's Asian joint venture customers. Ref: AirTicketRQ/OptionalQualifiers/PricingQualifiers/Fare/@Type</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="spanishLargeFamilyDiscount">
			<xsd:annotation>
				<xsd:documentation>Equivalent of secondary action code DFN which specifies the spanish large family discount level (EG: WDFN2 equatest to spanishLargeFamilyDiscount="2"). Used primarily by Spain-based subscribers. Ref: AirTicketRQ/OptionalQualifiers/PricingQualifiers/SpanishLargeFamilyDiscount</xsd:documentation>
			</xsd:annotation>
			<xsd:simpleType>
				<xsd:restriction base="xsd:string">
					<xsd:enumeration value="1"/>
					<xsd:enumeration value="2"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
		<xsd:attribute name="fareFocusExclude" type="xsd:boolean">
			<xsd:annotation>
				<xsd:documentation>Exclude focus fares based on travel agency business rules. Omission assumes false. Subscribers use this. Example: WXFF. Ref: AirTicketRQ/OptionalQualifiers/PricingQualifiers/FareFocusExclude/@Ind="true"</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="PricingOptionsItinerary">
		<xsd:sequence>
			<xsd:element name="Segment" type="SelectSegment" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Identify itinerary segments for pricing (EG: WS)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SideTrip" type="SideTripSegment" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Price particular itinerary segments as a side trip (EG: WST)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
