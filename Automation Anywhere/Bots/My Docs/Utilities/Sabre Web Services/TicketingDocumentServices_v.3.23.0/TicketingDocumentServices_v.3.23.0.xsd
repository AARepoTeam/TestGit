	<xsd:complexType name="Customer.Traveler">
		<xsd:sequence>
			<xsd:element name="Name" type="ProperName.29Character">
				<xsd:annotation>
					<xsd:documentation>The customer's full name.

                        Mapped to T2 database: TKT_DOC.CST_FUL_NM

                        Mapped to TPF file: #FS01_8020_NM1_001 (FS01_80_VAR)
                    </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="FirstName" type="ProperName.29Character" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The customer's first name.

                        Mapped to T2 database: TKT_DOC_CST.CST_FRST_NM;

                        Mapped to TPF file: #FS01_8020_NMG_004 (FS01_80_VAR)

                    </xsd:documentation>
				</xsd:annotation>
