	<xsd:complexType name="Ticketing.Update.ConsumeServiceCoupon">
		<xsd:sequence>
			<xsd:element name="Details" type="TicketingDocument.Details.Update" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Some detail information specific to a ticketing document</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Customer" type="TicketingDocument.Customer.Update" minOccurs="0"/>
			<xsd:element name="ServiceCoupon" type="TicketingDocument.ServiceCoupon.Update" maxOccurs="16">
				<xsd:annotation>
					<xsd:documentation>A ticketing document can have one or more flight coupons</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Remark" type="TicketingDocument.Remark.Update" minOccurs="0">
				<xsd:annotation>
