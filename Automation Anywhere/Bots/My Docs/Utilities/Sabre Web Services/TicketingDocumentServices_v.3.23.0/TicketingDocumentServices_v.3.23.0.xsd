	<xsd:complexType name="Ticketing.Details.History">
		<xsd:sequence>
			<xsd:element name="SupportingDocument" type="Identifier.TicketingDocument" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>True document identifier. It's often combined with the form, serial number, and
                        check digit number to make it complete.

                        Mapped to T2 database: TKT_DOC_HSTRY.TKT_DOC_ID
                    </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="OldReservation" type="PNR_Locator" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Old PNR Record Locator.

                        Mapped to VCR: VCR_D1_PNR

                        Mapped to T2 database: TKT_DOC_HSTRY.OLD_RES_ID
                    </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="NewReservation" type="PNR_Locator" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>New PNR Record Locator.

                        Mapped to VCR: VCR_D1_PN1

                        Mapped to T2 database:
                        TKT_DOC_HSTRY.NEW_RES_ID
                    </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="CouponBitMap" type="xsd:base64Binary" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>A technical bit map to show coupon usage.

                        Mapped to VCR: VCR_D1_CBM
                    </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="AccountingCode" type="Identifier.TicketingProviderAccounting" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>numeric identifier used to uniquely identify and airline for accounting purposes.

                        Mapped to T2 database: TKT_DOC_HSTRY.TKT_PROVD_ACCT_ID
                    </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="OldRemark" type="String.Long" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The previous remark text used on the document.

                        Mapped to T2 database: TKT_DOC_HSTRY.OLD_RMK_TXT
                    </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PreviousDocumentStatus" type="Code.DocumentStatus" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Represents the previous state of the Document.

                        Mapped to T2 database: TKT_DOC_HSTRY.PREV_DOC_STS_CD
                    </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DocumentPurgeTypeCode" type="Code.DocumentPurgeType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Mapped to T2 database: TKT_DOC_HSTRY.DOC_PRG_TYP_CD

                        Values can be:
                        AGE - Purge due to aging
                        TCN - Purge due to TCN
                        7DY - 7-Day Purge
                    </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="InConnectionDocument" type="Number.Document" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Mapped to T2 database:
                        TKT_DOC_HSTRY.IN_CONNX_WTH_DOC_ID

                        couponNUmber: TKT_DOC_HSTRY.IN_CONNX_WTH_CPN_NBR
                    </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="CurrentDocumentStatus" type="Code.DocumentStatus" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Represents the current state of the Document.

                        Mapped to T2 database: TKT_DOC_HSTRY.CUR_DOC_STS_CD
                    </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DocumentEventActivity" type="Code.DocumentEventActivityType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Represents the event that happens to the document when a transction is issued.

                        Mapped to T2 database: TKT_DOC_HSTRY.DOC_EVN_ACTV_CD
                    </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="AffectedCoupons" type="Number.CouponSeries" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Coupons affected by the transaction

                        Mapped to VCR: VCR_D1_CBM

                        Mapped to T2 database:
                        TKT_DOC_HSTRY.COUPONBITMAP
                    </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="Ticketing.Document">
		<xsd:sequence>
			<xsd:element name="Indicators" type="TicketingDocument.Indicators" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Ticketing document indicators with boolen values; default to 'false'
                    </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Details" type="TicketingDocument.Details">
				<xsd:annotation>
					<xsd:documentation>Some detail information specific to a ticketing document</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Customer" type="TicketingDocument.Customer">
				<xsd:annotation>
					<xsd:documentation>Traveller or purchaser</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Affinity" type="TicketingDocument.Affinity" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>A ticketing document can be associated to a travel loyalty</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ServiceCoupon" type="TicketingDocument.ServiceCoupon" minOccurs="0" maxOccurs="30">
				<xsd:annotation>
					<xsd:documentation>A ticketing document can have one or more flight coupons</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Miscellaneous" type="TicketingDocument.Miscellaneous" minOccurs="0" maxOccurs="16">
				<xsd:annotation>
					<xsd:documentation>Specific information about ancillary or miscellaneous items purchased. This
                        applies to MSR, EXB and EMD
                    </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Amounts" type="TicketingDocument.Amounts">
				<xsd:annotation>
					<xsd:documentation>Any amounts associated to the ticketing document</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Taxes" type="TicketingDocument.Taxes" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Any taxes paid by the customer</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Remark" type="TicketingDocument.Remark" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>type=ENRS (Endorsement and Restriction - Mapped to VCR_60_EDR, VCR_60_EDM,
                        VCR_60_RES; type=VCR5 (Manually-added remarks) - Mapped to VCR_80_MAR;

                        Mapped to T2 database: TKT_DOC_RMK.TKT_DOC_RMK_TXT

                        Mapped to: #FS01_8030_EDM_023 (FS01_80_VAR)
                    </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="AffiliatedAgent" type="TicketingDocument.AffiliatedAgent" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Other agents</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="FareCalculation" type="TicketingDocument.FareCalculation" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Mapped to T2 database: FARE_CALC.FARE_CALC_TXT

                        Mapped to: #FS01_8060_FCL_006 (FS01_80_VAR)

                        Mapped to DM0Y record:
                        DM0Y_95_DATA
                    </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="RelatedDocument" type="TicketingDocument.Related" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Other Documents that are related to the primary document upon issuance
                    </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PrintCoupon" type="TicketingDocument.PrintCoupon" minOccurs="0" maxOccurs="6">
				<xsd:annotation>
					<xsd:documentation>Coupon information that are printed as part of ticketing transaction
                    </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Payment" type="TicketingDocument.Payment" maxOccurs="3">
				<xsd:annotation>
					<xsd:documentation>Two forms of payment are allowed. In case of exchanges, there could be another
                        form of paymemt code 'ET' that signifies the use of travel documents to issue another ticketing
                        document.
                    </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="History" type="TicketingDocument.History" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>History information associated to every transaction update made to the ticketing
                        document
                    </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
		<xsd:attribute name="type" type="Code.DocumentType" use="required">
			<xsd:annotation>
				<xsd:documentation>Ticketing document type issued by the agent

                    Mapped to T2 database: TKT_DOC.TKT_DOC_TYP_CD

                    Mapped to TPF file: #FS01_8030_IND_000 (FS01_80_IND_DOC)

                    Mapped to VCR record: Always set the value to TKT

                    Possible values can be:
                    TKT - Ticket
                    EMD - Electronic Miscellaneous Document
                    MSR - Miscellaneous Sales Receipt
                    EXB - Excess Baggage
                    VOU - Voucher
                    RFD - Refund
                    XXX - Cash Refund
                    CCR - Credit Card Refund
                </xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="number" type="Identifier.TicketingDocument" use="required">
			<xsd:annotation>
				<xsd:documentation>Ticketing document number dispensed and assigned by the system upon issuance; this is
                    the primary document number

                    Mapped to T2 database:
                    TKT_DOC.TKT_DOC_ID

                    Mapped to TPF file: #FS01_8030_TKT_026 (FS01_80_VAR)

                    Mapped to VCR record: VCR_40_TKN and VCR_40_TKD

                    Mapped to DM0Y record:
                    DM0Y_50_TACC and DM0Y_50_TNUM
                </xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="accountingCode" type="Identifier.TicketingProviderAccounting">
			<xsd:annotation>
				<xsd:documentation>Airline code identification number as defined by IATA

                    Mapped to T2 database:
                    TKT_DOC.TKT_PROVD_ACCT_ID

                    Mapped to TPF file: #FS01_8030_TKT_026 (FS01_80_TKT_AAC)

                    Mapped to VCR record: VCR_40_TAC

                    Mapped to DM0Y record:
                    DM0Y_50_TACC
                </xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="formNumber" type="Number.TicketingDocumentForm">
			<xsd:annotation>
				<xsd:documentation>Number representing the type of document form as defined by IATA

                    Mapped to T2 database:
                    TKT_DOC.TKT_DOC_FORM_NBR;

                    Mapped to TPF file: #FS01_8030_TKT_026 (FS01_80_TKT_FMN)

                    Mapped to VCR record: VCR_40_FMN
                </xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="serialNumber" type="Number.TicketingDocumentSerial">
			<xsd:annotation>
				<xsd:documentation>Number assigned on a sequential basis to make the document number as unique as
                    possible

                    Mapped to T2 database: DB-TKT_DOC.TKT_DOC_SN

                    Mapped to TPF file: #FS01_8030_TKT_026 (FS01_80_TKT_TNR)

                    Mapped to VCR record: VCR_40_TNR

                    Mapped to DM0Y record:
                    DM0Y_50_TNUM
                </xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="checkDigit" type="Number.CheckDigit">
			<xsd:annotation>
				<xsd:documentation>A digit calculated based on the document number modulus 7 for fraud purposes

                    Mapped to T2 database:
                    TKT_DOC_CHK_DIGT_NBR

                    Mapped to TPF file: #FS01_8030_TKT_026 (FS01_80_TKT_TCD)

                    Mapped to VCR record: VCR_40_ACD

                    Mapped to DM0Y record:
                    DM0Y_55_TCKD (1st coupon)
                </xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="service" type="Code.ServiceType">
			<xsd:annotation>
				<xsd:documentation>A code associated to the general service category type to determine the reason why
                    the document is issued. The value could be related to A - AIR (VCR and non-VCR) or R - RAIL (AMTRAK)

                    Mapped to T2 database:
                    TKT_DOC.SVC_TYP_CD

                    Mapped to TPF file: #FS01_80_HDR_100 (FS01_80_HDR_TST)

                    Mapped to VCR record: Always set value to 'A'
                </xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
