	<xsd:complexType name="RelatedDocument.Validate">
		<xsd:sequence>
			<xsd:element name="Number" type="Identifier.TicketingDocument">
				<xsd:annotation>
					<xsd:documentation>Document Number

REF: TKT_REL_DOC.REL_DOC_ID</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ReasonForIssue" type="Code.TicketingDocumentReasonIssuanceCode_ATPCO_RFIC" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>A primary single characte code associated to the reason for issuance as defined
                        by IATA

                        Possible values can be:
                        A - Air Transporation
                        B _ Surface Transportation
                        C - Baggage
                        D - Financial Impact
                        E - Airport Services
                        F - Merchandise
                    </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ServiceCoupon" type="EMD.ServiceCoupon.Validate" minOccurs="0" maxOccurs="16">
				<xsd:annotation>
					<xsd:documentation>Service coupon information</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="UnusedAmount" type="Amount" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Amount of document or item that is unused.
                    </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="TotalAmount" type="Amount" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Total amount of the document or item.
                    </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
		<xsd:attribute name="refundable" type="Code.IATAIndicators">
			<xsd:annotation>
				<xsd:documentation>An IATA indicator representing whether the document is refundable.

                    Y = Yes (Service Type is refundable)
                    N = No (Service Type is non-refundable)
                    R = Re-use. (Service Type is non-refundable but the value may be reapplied towards a future
                    purchase)
                    Blank = No application (edits only permit Blank when the Service Type is not available)
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="sequence" type="xsd:integer">
			<xsd:annotation>
				<xsd:documentation>Mapped to T2 database:
                    REL_TKT_DOC.REL_TKT_DOC_SEQ
                </xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="type" type="Code.ElectronicMiscType">
			<xsd:annotation>
				<xsd:documentation>The type of the electronic miscellaneous document
                        Possible values can be:
                        A - Associated
                        S - Stand-alone
                    </xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
