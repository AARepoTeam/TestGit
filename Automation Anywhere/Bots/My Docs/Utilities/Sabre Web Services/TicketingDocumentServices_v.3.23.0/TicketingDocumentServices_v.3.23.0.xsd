		<xsd:attribute name="startLocation" type="STL:CityCode" use="required">
			<xsd:annotation>
				<xsd:documentation>Flight departure airport code.</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="endLocation" type="STL:CityCode" use="required">
			<xsd:annotation>
				<xsd:documentation>Flight arrival airport code.</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="bookingClass" type="String.Short" use="required">
			<xsd:annotation>
				<xsd:documentation>Reservation class code from PNR, must be provided is PNR is available and segment is still in the PNR.</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="reservationStatus" type="Code.BookingStatus" use="required">
			<xsd:annotation>
				<xsd:documentation>Reservation status from PNR, must be provided is PNR is available and segment is still in the PNR</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="bookingDateTime" type="xsd:dateTime">
			<xsd:annotation>
				<xsd:documentation>The date and time itinerary items was booked and stored in PNR</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="RelatedServiceCoupon">
		<xsd:sequence>
			<xsd:element name="SequenceNumber" type="Number.CouponSequence">
				<xsd:annotation>
					<xsd:documentation>Coupon Number- will be converted from hex to character format -
                        1,2,3,4,5,6,7,8,9,etc.&#13;
                        Sequence number of a coupon. Maximum coupon number is 4. Sabre can only issue up to 30 coupons
                        on a single ticket. 16 segment max for ATB1 or ATB2 for host airline issued tickets and 30 max
                        for carbonized tickets. A coupon cannot be renumbered.
                    </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="FeeOwningProvider" type="Code.Ticketing">
				<xsd:annotation>
					<xsd:documentation>A system of letters into which normal language is converted to allow information
                        to be communicated briefly into a Owning Fee Provider Code&#13;
                        &#13;
                        This is the carrier that owns the fees associated with a miscellanous item on a miscellaneous
                        document.&#13;
                        &#13;
                        BA operates and uses AA misc item pricing.
                    </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="relatedGroupRequest">
		<xsd:sequence>
			<xsd:choice minOccurs="0">
				<xsd:element name="Ticket" type="IssueTicket" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation>Initial version corresponds directly with AirTicketLLSRQ. Future versions may go to PSS directly.</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
			</xsd:choice>
			<xsd:element name="Payment" type="paymentRQDetails" minOccurs="0" maxOccurs="2">
				<xsd:annotation>
					<xsd:documentation>xPaths given for AirTicketLLSRQ (ATXP) in annotations assume single form of payment for airline ticketing. For Airlines - In case of MFOP if any of securityCode, extendedPayment or expiryDate is present, this will map to EnhancedMultipleFOP or it will map to MultipleFOP in SabreSonicTicketing. For TN ticketing - In case of MFOP this can be mapped to MultipeFOP or FOP under BSPTicketing.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
		<xsd:attribute name="index" type="xsd:integer">
			<xsd:annotation>
				<xsd:documentation>Transaction index</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="endTransaction" type="xsd:boolean">
			<xsd:annotation>
				<xsd:documentation>Assume false if omitted. If true, generates EW instead of the usual W at the start of the entry causing the PNR to be ended prior to beginning the ticket issuance process. Ref: AirTicketRQ/OptionalQualifiers/MiscQualifiers/EndTransaction/@Ind="true"</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="returnHostCommand" type="xsd:boolean">
			<xsd:annotation>
				<xsd:documentation>Set to true to show the host command used to make the request be returned in the response. Assume false if this attribute is omitted. Ref: AirTicketRQ/@ReturnHostCommand="true"</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="numResponses" type="xsd:integer">
			<xsd:annotation>
				<xsd:documentation>numResponses is mandatory in AirticketRQ. So a default value of 1 may be used. 
			Ref: AirTicketRQ/@NumResponses</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="relatedGroupResponse">
		<xsd:sequence>
			<xsd:element name="Ticket" type="ticketResponse" minOccurs="0" maxOccurs="99">
				<xsd:annotation>
					<xsd:documentation>Results from call to ticket service. Multiple responses may occur for TN customers.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
		<xsd:attribute name="index" use="required">
			<xsd:annotation>
				<xsd:documentation>Transaction index which matches respective requested transaction index</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="Remark.Details">
		<xsd:simpleContent>
			<xsd:extension base="String.TicketingRemark">
				<xsd:attribute name="sequence" type="xsd:integer" use="required">
					<xsd:annotation>
						<xsd:documentation>Mapped to T2 database: TKT_DOC_RMK.TKT_DOC_RMK_SEQ</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
			</xsd:extension>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="Remark.DetailsWithHeader">
		<xsd:simpleContent>
			<xsd:extension base="String.TicketingRemark">
				<xsd:attribute name="sequence" type="xsd:integer" use="required">
					<xsd:annotation>
						<xsd:documentation>Mapped to T2 database: TKT_DOC_RMK.TKT_DOC_RMK_SEQ</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
				<xsd:attribute name="header" type="String.TicketingRemark">
					<xsd:annotation>
						<xsd:documentation>Header text obtained from a control text table</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
			</xsd:extension>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="Remark.DetailsWithType">
		<xsd:simpleContent>
			<xsd:extension base="String.TicketingRemark">
				<xsd:attribute name="sequence" type="xsd:integer" use="required">
					<xsd:annotation>
						<xsd:documentation>Remark sequence number</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
				<xsd:attribute name="type" type="Code.RemarkType" use="optional">
					<xsd:annotation>
						<xsd:documentation>Remark type - e.g. FOP1, FOP2, etc.</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
			</xsd:extension>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="Remark.DetailsWithType.History">
		<xsd:complexContent>
			<xsd:extension base="Remark.DetailsWithType">
				<xsd:attribute name="action" use="required">
					<xsd:annotation>
						<xsd:documentation>Specifies action to be taken with the specified remark:

                            set - add a new remark, or update existing remark with the same type and sequence.

                            remove - remove the remark.
                        </xsd:documentation>
					</xsd:annotation>
					<xsd:simpleType>
						<xsd:restriction base="xsd:string">
							<xsd:enumeration value="set"/>
							<xsd:enumeration value="remove"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:attribute>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="Remark.DetailsWithType.Remove">
		<xsd:attribute name="sequence" type="xsd:integer">
			<xsd:annotation>
				<xsd:documentation>Remark sequence number (reserved for future use). If not provided, system will assume
                    default sequence number for given remark type.
                </xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="type" type="Code.RemarkType" use="required">
			<xsd:annotation>
				<xsd:documentation>Remark type - e.g. FOP1, FOP2, etc.</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="Remark.DetailsWithType.Set">
		<xsd:simpleContent>
			<xsd:extension base="String.TicketingRemark">
				<xsd:attribute name="sequence" type="xsd:integer">
					<xsd:annotation>
						<xsd:documentation>Remark sequence number (reserved for future use). If not provided, system
                            will assume default sequence number for given remark type.
                        </xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
				<xsd:attribute name="type" type="Code.RemarkType" use="required">
					<xsd:annotation>
						<xsd:documentation>Remark type - e.g. FOP1, FOP2, etc.</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
			</xsd:extension>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="Reservation.Details">
		<xsd:sequence>
			<xsd:element name="Sabre" type="Identifier.Reservation" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Reservation information as generated in the host system

                        Mapped to T2 database: TKT_DOC.SAB_RES_ID
                        TKT_DOC.RES_PROVD_CD --- provider
                        TKT_DOC.RES_PRG_DTM --- purgeDate
                        TKT_DOC.RES_CRE_DTM --- createDate

                        Mapped to TPF File: #FS01_8030_PNR_018 (FS01_80_PNR_LOC)
                        #FS01_8030_PNR_018 (FS01_80_PNR_CCP) --- provider
                        #FS01_8030_DT2_072 (FS01_80_DAT_DTE, FS01_80_DAT_TIM) --- purgeDate
                        #FS01_8030_PDT_068 (FS01_80_DAT_DTE, FS01_80_DAT_TIM) --- createDate

                        Mapped to VCR record: VCR_40_PNR
                        VCR_40_CCP --- provider
                        VCR_40_DT2 --- purgeDate
                        VCR_40_DT1 --- createDate

                        Mapped to DM0Y record:
                        DM0Y_45_PNRL
                        DM0Y_45_PNRC - provider
                        DM0Y_45_PRPD - purgeDate
                        DM0Y_45_PRCD - createDate


                    </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="OtherSystem" type="Identifier.Reservation" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Reservation information provided by other system

                        Mapped to T2 database: TKT_DOC.OTH_RES_ID

                        Mapped to TPF file:
                        #FS01_8030_EPN_122 (FS01_80_PNR_LOC)

                        Mapped to VCR record: VCR_40_CRP

                        Mapped to DM0Y record:
                    </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="Reservation.Details.Edd">
		<xsd:sequence>
			<xsd:element name="Sabre" type="Identifier.Reservation" minOccurs="0"/>
			<xsd:element name="OtherSystem" type="Identifier.Reservation" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="Reservation.Details.Pir">
		<xsd:sequence>
			<xsd:element name="Sabre" type="Identifier.Reservation">
				<xsd:annotation>
					<xsd:documentation>Reservation information as generated in the host system</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="Reservation.SearchType">
		<xsd:sequence>
			<xsd:group ref="LocalIssueDateChoice.CG" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attribute name="pnrLocator" type="PNR_Locator" use="required"/>
		<xsd:attribute name="pnrCreateDate" type="xsd:date"/>
		<xsd:attribute name="workLocation" type="Identifier.LocationExtended"/>
	</xsd:complexType>
	<xsd:complexType name="Reservation.Type">
		<xsd:simpleContent>
			<xsd:extension base="PNR_Locator">
				<xsd:attribute name="createDate" type="xsd:dateTime">
					<xsd:annotation>
						<xsd:documentation>The date when the reservation has been made

                            Mapped to T2 database: TKT_DOC.
                        </xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
			</xsd:extension>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="Reservation.Validation">
		<xsd:sequence>
			<xsd:element name="Sabre" type="PNR_Locator">
				<xsd:annotation>
					<xsd:documentation>Sabre PNR locator.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="SelectNameType">
		<xsd:attribute name="number" type="xsd:string" use="required">
			<xsd:annotation>
				<xsd:documentation>Identify the name by number (EG: ...N1.1 equates to SelectName nameNumber="1.1")</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="endNumber" type="xsd:string">
			<xsd:annotation>
				<xsd:documentation>Identify the names starting with nameNumber and ending with endNameNumber (EG: ...N1.1-3.2 equates to SelectName nameNumber="1.1" endNameNumber="3.2")</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="SelectSegment">
		<xsd:attribute name="number" type="xsd:integer" use="required">
			<xsd:annotation>
				<xsd:documentation>Price the segment as identified by number. EG1: WS1 equates to Segment number="1". EG2: WV1 equates to Validity number="1".</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="endNumber" type="xsd:integer">
			<xsd:annotation>
				<xsd:documentation>Price the segments as identified starting with number and ending with endNumber. EG1: WS1-5 equates to Segment number="1" endNumber="5". EG2: WV1-5 equates to Validity number="1" endNumber="5".</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="segmentIndex" type="xsd:integer">
			<xsd:annotation>
				<xsd:documentation>Used to correlate Brand and selected itinerary segment. Ref: AirTicketRQ/OptionalQualifiers/PricingQualifiers/ItineraryOptions/SegmentSelect/@RPH</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="ServiceCoupon.Indicators">
		<xsd:attribute name="fareBreak" type="xsd:boolean">
			<xsd:annotation>
				<xsd:documentation>A boolean (Y/N) indicator which identifies the fare break point in the service coupon

                    Mapped to T2 datbase: SVC_CPN.FARE_BREAK_IND

                    Mapped to TPF file: #FS01_8040_IND_000 (FS01_80_SEG_IND, #FS01_80_SEG_BRK)

                    Mapped to VCR record:
                    VCR_48_FARI (#VCR_48_BRK)

                    Mapped to DM0Y record:
                    DM0Y_55_FARI
                    #DM0Y_55_FBK

                </xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="turnaround" type="xsd:boolean">
			<xsd:annotation>
				<xsd:documentation>Mapped to DM0Y record:
                    DM0Y_55_FARI
                    #DM0Y_55_TAP

                </xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="noBreak" type="xsd:boolean">
			<xsd:annotation>
				<xsd:documentation>Mapped to DM0Y record:
                    DM0Y_55_FARI
                    #DM0Y_55_NBK

                </xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="connectionOverride" type="xsd:boolean">
			<xsd:annotation>
				<xsd:documentation>Mapped to DM0Y record:
                    DM0Y_55_FARI
                    #DM0Y_55_FSN

                </xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="zeroFareAmount" type="xsd:boolean">
			<xsd:annotation>
				<xsd:documentation>Zero Fare Amount Indicator
                    Mapped to:
                    VCR_48_FARI,#VCR_48_ZER

                    Mapped to T2:
                    ZERO_FARE_AMT_IND
                </xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="unchargeableSurface" type="xsd:boolean">
			<xsd:annotation>
				<xsd:documentation>
                    Mapped to DM0Y record:
                    DM0Y_55_FARI
                    #DM0Y_55_USS

                    Mapped to:
                    VCR_48_FARI#VCR_48_SUR

                    Mapped to T2:
                    UNCHRGBL_SURF_IND
                </xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="sideTripStart" type="xsd:boolean">
			<xsd:annotation>
				<xsd:documentation>Mapped to DM0Y record:
                    DM0Y_55_FARI
                    #DM0Y_55_STS
                    Mapped to:
                    VCR_48_FARI,#VCR_48_STS

                    Mapped to T2:
                    SIDE_TRIP_STR_IND
                </xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="sideTripEnd" type="xsd:boolean">
			<xsd:annotation>
				<xsd:documentation>Mapped to DM0Y record:
                    DM0Y_55_FARI
                    #DM0Y_55_STE

                    Mapped to:
                    VCR_48_FARI,#VCR_48_STE

                    Mapped to T2:
                    SIDE_TRIP_END_IND
                </xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="ServiceCoupon.InvoluntaryReroute">
		<xsd:sequence>
			<xsd:element name="Reason" type="Code.InvoluntaryReason" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Code identifying the reason for the involuntary reroute

                        Mapped to T2 database:
                        SVC_CPN.INVOL_REROUTE_RSN_CD

                        Mapped to VCR record:
                        VCR_48_INV
                    </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="RerouteDescription" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Description of Reason code

						Possible values can be:
						L - STRIKE
						S - SCHEDULE CHANGE
						I - INVOL
						D - DIVERSION
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Date" type="xsd:date" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The date for the involuntary reroute

                        Mapped to T2 database: SVC_CPN.INVOL_REROUTE_DT

                        Mapped to TPF file: #FS01_8040_IDT_043 (FS01_80_DAT_DTE)

                        Mapped to VCR record:
                        VCR_48_IDT (pars date)
                    </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Provider" type="Code.TicketingProviderCode" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Involuntary reroute airline code

                        Mapped to T2 database:
                        SVC_CPN.INVOL_PROVD_CD

                        Mapped to TPF file:
                        '#FS01_8040_ICC_044 (FS01_80_ALN_ALC)

                        Mapped to VCR record:
                        VCR_48_ICC
                    </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="ServiceCoupon.Search">
		<xsd:sequence>
			<xsd:element name="StartLocation" type="STL:CityCode" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The origin city of the issued documentRef: SVC_CPN. SCTY_CD  EX: LHR					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EndLocation" type="STL:CityCode" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The destination city of the issued document Ref: SVC_CPN. SVC_END_CTY_CD Ex: AUH					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="LastName" type="ServiceCouponLastName.Search" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Passenger Name Details				 Ref:				 TKT_DOC_CST					   </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="MarketingFlight" type="Flight.Search" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Flight information data				 Ref:  SVC_CPN					   </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:group ref="FlightStartDateChoice.CG" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attribute name="status" type="Code.ServiceCouponStatus">
			<xsd:annotation>
				<xsd:documentation>				    Current Service Coupon Status				 Ref:   SVC_CPN. CUR_SVC_CPN_STS_CD					   					</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="openSegmentInd" type="xsd:boolean">
			<xsd:annotation>
				<xsd:documentation>				 OPEN segment indicator				 Ref: SVC_CPN.SEG_TYP_CD					   					</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="ServiceCouponLastName.Search">
		<xsd:attribute name="lastName" type="ProperName.29Character" use="required">
			<xsd:annotation>
				<xsd:documentation>Last Name of the pasenger				 Ref:				 TKT_DOC_CST.CST_LAST_NM 					   </xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="firstName" type="ProperName.29Character">
			<xsd:annotation>
				<xsd:documentation>First Name of the pasenger				 Ref:				 TKT_DOC_CST.CST_FRST_NM 					   </xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="Flight.Search">
		<xsd:attribute name="number" type="STL:FlightNumber" use="required">
			<xsd:annotation>
				<xsd:documentation> Marketing Flight Number  				 Ref: SVC_CPN. MKT_FLT_NBR			   					</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="provider" type="Code.TicketingProviderCode">
			<xsd:annotation>
				<xsd:documentation>Marketing Provider Code				  Ref:        SVC_CPN. MKT_PROVD_CD				  Ex: EY	   					</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="FormOfIdentification.Search">
		<xsd:sequence>
			<xsd:group ref="FlightStartDateChoice.CG" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attribute name="type" type="Code.FOID" use="required">
			<xsd:annotation>
				<xsd:documentation>					 Form of Identification type code							Ref:   CST_FORM_OF_ID. FORM_OF_ID_TYP_CD					   					</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="number" type="Identifier.FOID" use="required">
			<xsd:annotation>
				<xsd:documentation>					 Form of Identification number							   Ref:	CST_FORM_OF_ID. PRSNL_ID_ID					   					</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="country" type="STL:CountryCode">
			<xsd:annotation>
				<xsd:documentation>					 Form of Identification country code				  Ref:		   CST_FORM_OF_ID.FORM_OF_ID_CNTRY_CD					   					</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="FrequentFlyer.Search">
		<xsd:sequence>
			<xsd:group ref="FlightStartDateChoice.CG" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attribute name="number" type="Identifier.Affinity" use="required">
			<xsd:annotation>
				<xsd:documentation>						The customer's loyalty number assigned by the airline     Ref:			AFF_ACCT.AFF_ACCT_ID and AFF_ACCT.AFF_ACCT_TYP_CD					</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="provider" type="Code.TicketingProviderCode">
			<xsd:annotation>
				<xsd:documentation>					 Frequent flyer provider code				  Ref:		   AFF_ACCT.AFF_PROVD_CD					   					</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="Card.Search">
		<xsd:sequence>
