		<xsd:attribute name="paymentConfirmation" type="xsd:boolean">
			<xsd:annotation>
				<xsd:documentation>Indicates that payment confirmation needs to be sent to Payment System after
                    successful transaction

                    Mapped to T2 Database
                    TKT_REL_DOC.PAYMNT_CONF_IND

                    Mapped to FS01 Record:
                    VCR_78_IN1#BIT 2  ON = Payment conf(Orig doc)
                </xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
